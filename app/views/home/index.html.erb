  <div class="container">
    <div class="d-flex justify-content-between align-items-center mb-2">
      <h5 class="mb-0">Products For You</h5>
      <p class="mb-0"><a href="<%= all_products_path %>">See more</a></p>
    </div>
  </div>

  <div class="container" style="overflow-x: auto; white-space: nowrap; width: 90%;">
    <% @products.first(8).each do |product| %>
      <div class="mr-2" style="display: inline-block; position: relative; width: 180px;">
        <%= link_to product_path(product) do %>
          <div class="card" style="width: 170px; height: 220px; position: relative;">
            <div class="card-body" style="overflow: hidden; text-overflow: ellipsis; box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);">
              <h5 class="card-title" style="font-size: 16px; white-space: normal;"><strong><%= product.name %></strong></h5>
              <p class="card-text" style="font-size: 14px;"><%= product.weight %>grms</p>
              <div style="display: flex; align-items: center;">
                <h5 class="card-text" style="font-size: 16px;"><strong><%= number_to_currency(product.price, unit: 'â‚¹') %></strong></h5>
              </div>
            </div>
          </div> 
        <% end %>
        <div style="position: absolute; bottom: 20px; left: 70%; transform: translateX(-50%);">
          <div id="add-to-cart-btn-<%= product.id %>" >
            <%= form_for :product, url: "/products/#{product.id}/add_to_cart", method: :post, remote: true do |f| %>
              <%= f.submit "Add", class: "btn btn-danger btn-lg text-decoration-none custom-btn"%>
            <% end %>
          </div>
          <div id="quantity-buttons-<%= product.id %>"></div>
        </div>
      </div>
    <% end %>
  </div>


  <div class="container">
    <div class="d-flex justify-content-between align-items-center mb-2">
      <h5 class="mb-2">Explore By Categories</h5>
      <p class="mb-0"><a href="<%= all_categories_path %>">See more</a></p>
    </div>
  </div>

  <div class="container">
    <div class="row">
      <% @categories.each_slice(7) do |category_slice| %>
        <div class="row">
          <% category_slice.each do |category| %>
            <div class="col-2">
              <a href="<%= category_path(category) %>">
                <div class="card category-container" style="width: 100%; height: 150px;"> 
                  <div class="card-body">
                  </div>
                </div>
                <h5><strong><%= category.name %></strong></h5>
              </a>
            </div>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>


  <div class="container">
    <% @products.each_slice(5) do |product_slice| %>
      <div class="row">
        <%= product_slice.each do |product| %>
          <div class="col-2 border border-2">
            <h5><strong><%= product.name %></strong></h5>
            <p>Price: <%= number_to_currency(product.price) %></p>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>

